RewriteEngine On
RewriteRule   palloo   palloo.php
RewriteRule   index   index.php

ErrorDocument 404 /404.php
ErrorDocument 403 /403.php
ErrorDocument 500 /500.php

RedirectMatch 404 /\.git

<Files ~ "\.(json|html|py|pyw|pyc|log)$">
  Order deny,allow
  Deny from all
</Files>

Options All -Indexes
